<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PlayStoreCollection" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
<!--        <item name="colorPrimary1">@color/blue_light</item>-->
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/orange1</item>
        <item name="colorTertiary">@color/blue_light1</item>
        <item name="colorSecondaryVariant">@color/black</item>
        <item name="colorOnSecondary">@color/blue</item>
<!--        <item name="toolbarColor">@color/blue</item>-->
        <item name="colorOnContainer">@color/blue_light</item>
        <item name="colorOnPrimaryFixed">@color/sky_blue</item>
        <item name="colorOnSecondaryFixed">@color/gray_light</item>
        <item name="colorSurface">@color/black_new</item>
        <item name="dividerInsetEnd">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.PlayStoreCollection.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.PlayStoreCollection.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.PlayStoreCollection.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>